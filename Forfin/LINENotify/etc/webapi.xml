<?xml version="1.0" ?>
<routes xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Webapi:etc/webapi.xsd">
	<route method="POST" url="/V1/forfin-linenotify/linenotifylogs">
		<service class="Forfin\LINENotify\Api\LineNotifyLogsRepositoryInterface" method="save"/>
		<resources>
			<resource ref="Forfin_LINENotify::LineNotifyLogs_save"/>
		</resources>
	</route>
	<route method="GET" url="/V1/forfin-linenotify/linenotifylogs/search">
		<service class="Forfin\LINENotify\Api\LineNotifyLogsRepositoryInterface" method="getList"/>
		<resources>
			<resource ref="Forfin_LINENotify::LineNotifyLogs_view"/>
		</resources>
	</route>
	<route method="GET" url="/V1/forfin-linenotify/linenotifylogs/:linenotifylogsId">
		<service class="Forfin\LINENotify\Api\LineNotifyLogsRepositoryInterface" method="get"/>
		<resources>
			<resource ref="Forfin_LINENotify::LineNotifyLogs_view"/>
		</resources>
	</route>
	<route method="PUT" url="/V1/forfin-linenotify/linenotifylogs/:linenotifylogsId">
		<service class="Forfin\LINENotify\Api\LineNotifyLogsRepositoryInterface" method="save"/>
		<resources>
			<resource ref="Forfin_LINENotify::LineNotifyLogs_update"/>
		</resources>
	</route>
	<route method="DELETE" url="/V1/forfin-linenotify/linenotifylogs/:linenotifylogsId">
		<service class="Forfin\LINENotify\Api\LineNotifyLogsRepositoryInterface" method="deleteById"/>
		<resources>
			<resource ref="Forfin_LINENotify::LineNotifyLogs_delete"/>
		</resources>
	</route>
</routes>
